<UserControl x:Class="TouchChanX.Ava.Menu.Pages.GamePage"
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:menu="using:TouchChanX.Ava.Menu"
    xmlns:pages="using:TouchChanX.Ava.Menu.Pages"
    d:DesignHeight="300"
    d:DesignWidth="300"
    mc:Ignorable="d">

    <Grid x:Name="GamePageGrid" ColumnDefinitions="*, *, *" RowDefinitions="*, *, *">
        <menu:MenuItem
            Grid.Row="1"
            Grid.Column="0"
            Symbol="Setting"
            Text="Setting" />
    </Grid>
</UserControl>
